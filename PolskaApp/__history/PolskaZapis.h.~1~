//---------------------------------------------------------------------------

#ifndef PolskaZapisH
#define PolskaZapisH
#include "Stack.h"
#include <vcl.h>
//---------------------------------------------------------------------------
#endif

using namespace std;

struct PolskaZapis
{
	String note;
	String polskNote;

	void convert()
	{
		int f = note.Length();
		for (int i = 0; i < f; i++)
		{
			if (isspace(note[i]))
			{
				note = note.Delete(i,1);
			}
		}
    }
};